<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewInfo"
    android:background="@drawable/bg11">

    <TextView
        android:id="@+id/editTextCattleBreed"

        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:ems="10"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center"
        />




    <TextView
        android:id="@+id/TextViewCattleTagId"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleDOB"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleSpeacialFeature"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleSex"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="250dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleNoOfLactation"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="300dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewFarmBirthWeight"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="350dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleBreedingWeight"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="400dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleWeaningWeight"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="450dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleAveragePreWeaningGrowthRate"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="500dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/TextViewCattleAveragePostWeaningGrowthRate"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="550dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/editTextCattleLastCalvingDate"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="600dp"
        android:ems="10"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/editTextCattleFarmName"
        android:background="@drawable/edittextstyle"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="650dp"
        android:ems="10"
        android:gravity="center"

        />





</RelativeLayout>
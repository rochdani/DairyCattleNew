<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg11"
    tools:context=".ViewCattles">


    <TextView
        android:id="@+id/farmGSDiv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmAddress"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmVetDiv" />

    <TextView
        android:id="@+id/FarmName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="65dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmRegNo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/farmAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmContactNo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmGSDiv" />

    <TextView
        android:id="@+id/farmOwnName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmVetDiv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmRegNo" />

    <TextView
        android:id="@+id/farmVetDiv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmGSDiv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmOwnName" />

    <TextView
        android:id="@+id/farmCattleCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="4dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmDairyCattleCount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmContactNo" />

    <TextView
        android:id="@+id/farmContactNo"
        android:layout_width="75dp"
        android:layout_height="23dp"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmCattleCount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmAddress" />

    <TextView
        android:id="@+id/farmDairyCattleCount"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="47dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/ListViewCattle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/farmCattleCount" />

    <TextView
        android:id="@+id/farmRegNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textColor="@color/colorPrimaryDark1"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/farmOwnName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FarmName" />

    <ListView
        android:id="@+id/ListViewCattle"
        android:layout_width="400dp"
        android:layout_height="450dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/buttonshape1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonAddCattle"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="172dp"
        android:background="@drawable/add1"
        app:layout_constraintBottom_toTopOf="@+id/ListViewCattle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.142" />

</androidx.constraintlayout.widget.ConstraintLayout>